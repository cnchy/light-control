<view class="page" style="height:100vh;"><cu-custom vue-id="1" bgImage="{{bg_titlebar}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">色温参数设置</view></cu-custom><block wx:for="{{tempretureMap}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="room"><view class="title"><view class="light-no">{{''+key+''}}</view><view class="light-degree"><yp-number-box vue-id="{{'2-'+key}}" min="0" max="999" value="{{value[0]}}" data-event-opts="{{[['^change',[['changeArgs',['$event',key,0]]]]]}}" bind:change="__e" bind:__l="__l"></yp-number-box></view><view class="light-degree"><yp-number-box vue-id="{{'3-'+key}}" min="0" max="999" value="{{value[1]}}" data-event-opts="{{[['^change',[['changeArgs',['$event',key,1]]]]]}}" bind:change="__e" bind:__l="__l"></yp-number-box></view><view class="light-degree"><yp-number-box vue-id="{{'4-'+key}}" min="0" max="999" value="{{value[2]}}" data-event-opts="{{[['^change',[['changeArgs',['$event',key,2]]]]]}}" bind:change="__e" bind:__l="__l"></yp-number-box></view><view class="light-degree"><yp-number-box vue-id="{{'5-'+key}}" min="0" max="999" value="{{value[3]}}" data-event-opts="{{[['^change',[['changeArgs',['$event',key,3]]]]]}}" bind:change="__e" bind:__l="__l"></yp-number-box></view><view class="light-degree"><yp-number-box vue-id="{{'6-'+key}}" min="0" max="999" value="{{value[4]}}" data-event-opts="{{[['^change',[['changeArgs',['$event',key,4]]]]]}}" bind:change="__e" bind:__l="__l"></yp-number-box></view></view></view></block><view class="room" style="text-align:center;display:flex;justify-content:space-around;"><button data-event-opts="{{[['tap',[['saveDegree']]]]}}" class="cu-btn round" bindtap="__e">保存</button><navigator class="cu-btn round" style="color:#000000;" url="/pages/index/index">返回</navigator></view></view>