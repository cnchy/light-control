.room{
	width: 98%;
	margin: 20rpx auto;
	padding: 10rpx;
	position: relative;
	border-radius: 2px;
	box-shadow: 0 2px 10px rgba(0,0,0,.12), 0 2px 5px rgba(0,0,0,.16);
	background-image: -webkit-linear-gradient(bottom left, #6F737C, #34363A);
	background-image: linear-gradient(to top right, #6F737C, #34363A);
	color: white;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
}
.light-no{
	height:40rpx;
	width: 160rpx;
	left: 10rpx;
}
.light-degree{
